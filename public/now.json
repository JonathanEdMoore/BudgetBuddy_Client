{
  "version": 2,
  "alias": "budgetbuddy-app",
  "name": "capstone_1_budgetbuddy_client",
  "routes": [
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1"
    },
    { "src": "^/favicon.ico$",
      "dest": "/favicon.ico"
    },
    {
      "src": "^/manifest.json$",
      "dest": "/manifest.json"
     },
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ]
}